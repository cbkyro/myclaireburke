{"components/cart/content":"<!--\r\n  Gift wrap:\r\n    {{!> components/cart/item-gift-wrap}}\r\n-->\r\n\r\n{{#each cart.items}}\r\n  <article class=\"cart-product-row\" data-cart-item data-item-id=\"{{id}}\">\r\n    <figure class=\"cart-product-figure\">\r\n      {{#if type '===' 'GiftCertificate'}}\r\n        <div class=\"gift-card-image\">\r\n          <svg version=\"1.1\" id=\"Layer_1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" x=\"0px\" y=\"0px\"\r\n             viewBox=\"0 0 100 81.9\" style=\"enable-background:new 0 0 100 81.9;\" xml:space=\"preserve\">\r\n            <path style=\"fill:currentColor\" d=\"M77.4,0.3L5.8,21.1c-4.3,1.2-6.7,5.7-5.5,10l12.3,42.3c1.2,4.3,5.7,6.7,10,5.5l71.6-20.8\r\n              c4.3-1.2,6.7-5.7,5.5-10L87.4,5.8C86.2,1.5,81.7-0.9,77.4,0.3z M84.1,6.8l2.6,8.8L48.1,26.8c-0.7-2.2-1.8-5.4-3.2-7.6\r\n              c0,0-7.6,1.7-11,5.6l-2.2-7.6L78.4,3.6C80.8,2.9,83.4,4.3,84.1,6.8z M6.8,24.5l17.4-5.1l2.4,8.1c-5.1-0.3-11.4,3.9-11.4,3.9\r\n              c0.4,1.5,1,3,1.6,4.5L6.2,39l-2.6-8.8C2.9,27.7,4.3,25.2,6.8,24.5z M15.9,72.5l-7.6-26l17.1-5c-0.8,2.5-2,5.5-3.6,9.1l5.2,9.4\r\n              c0,0,2.5-7.1,4.7-14.7l7.3,25.3l-17.4,5.1C19.2,76.3,16.6,74.9,15.9,72.5z M93.3,54.8L46.5,68.4l-8.3-28.5C45.9,46,55,52.5,55,52.5\r\n              L53,42c-3.4-2.3-6.2-4.3-8.4-6l44.3-12.9l7.6,26C97.1,51.5,95.7,54.1,93.3,54.8z\"/>\r\n            <ellipse style=\"fill:currentColor\" cx=\"53.6\" cy=\"79.3\" rx=\"41.3\" ry=\"2.6\"/>\r\n          </svg>\r\n        </div>\r\n      {{else}}\r\n        <a href=\"{{url}}\">\r\n          <img\r\n            src=\"{{getImage image 'medium' ../../theme_settings.default_image}}\"\r\n            alt=\"{{image.alt}}\"\r\n            title=\"{{image.alt}}\"\r\n          >\r\n        </a>\r\n      {{/if}}\r\n    </figure>\r\n\r\n    <div class=\"cart-product-details\">\r\n      <h2 class=\"cart-product-name\">\r\n        <a href=\"{{url}}\">\r\n          {{name}}\r\n        </a>\r\n        <span class=\"cart-price-per\">\r\n          ({{price.formatted}})\r\n        </span>\r\n      </h2>\r\n      <div class=\"cart-product-brand\">{{brand.name}}</div>\r\n      {{#if options.length}}\r\n        <dl class=\"cart-item-options\">\r\n          {{#each options}}\r\n            <dt>{{name}}: </dt>\r\n            <dd>{{value}}</dd>\r\n          {{/each}}\r\n        </dl>\r\n      {{/if}}\r\n      <div class=\"cart-gift-wrap\">\r\n        {{> components/cart/gift-wrap-item this}}\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"cart-product-quantity\">\r\n      <span class=\"quantity-text\">{{lang 'cart.quantity'}}</span>\r\n\r\n      {{> components/common/quantity this}}\r\n\r\n      <div class=\"quantity-update\">\r\n        <span data-cart-item-update>{{lang 'cart.update'}}</span>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"cart-product-price\">\r\n      <span class=\"{{#if price_discounted}}price-discounted{{/if}}\">{{total.formatted}}</span>\r\n      {{#if price_discounted}}\r\n        <span class=\"cart-item-discount\">{{price_discounted.formatted}}</span>\r\n      {{/if}}\r\n    </div>\r\n\r\n    <div class=\"cart-product-remove\">\r\n      <a href=\"{{remove_url}}\" data-cart-item-remove>&times;</a>\r\n    </div>\r\n\r\n  </article>\r\n{{/each}}\r\n","components/cart/gift-wrap-item":"{{inject 'removeGiftWrap' (lang 'cart.gift_wrapping.remove_confirm')}}\n\n{{#if show_gift_wrapping}}\n  <dl class=\"gift-wrap\">\n    {{#if gift_wrapping}}\n      <dt>{{lang 'cart.gift_wrapping.title'}}</dt>\n      <dd>\n        {{gift_wrapping.name}}\n        <span>\n          (<a href=\"#\" data-item-giftwrap=\"{{id}}\">{{lang 'cart.gift_wrapping.change'}}</a>\n          {{lang 'cart.gift_wrapping.change_or'}}\n          <a href=\"{{gift_wrapping.remove_url}}\" data-giftwrap-remove>\n            {{lang 'cart.gift_wrapping.remove'}}\n          </a>)\n        </span>\n      </dd>\n      {{#if gift_wrapping.message}}\n        <dt>{{lang 'cart.gift_wrapping.gift_message'}}</dt>\n        <dd>{{gift_wrapping.message}}</dd>\n      {{/if}}\n    {{else}}\n      <dt>{{lang 'cart.gift_wrapping.title'}}</dt>\n      <dd><a href=\"#\" data-item-giftwrap=\"{{id}}\">{{lang 'cart.gift_wrapping.add'}}</a></dd>\n    {{/if}}\n  </dl>\n{{/if}}\n","components/common/quantity":"{{!\n  Quantity Widget with Increment/Decrement controls.\n\n  @param product\n         cart.item or product object\n}}\n\n<span class=\"form-field-quantity-control\" data-quantity-control=\"{{id}}\">\n  <input\n    class=\"form-input\"\n    type=\"number\"\n    name=\"qty{{#if product_id}}-{{id}}{{else}}[]{{/if}}\"\n    pattern=\"[0-9]*\"\n    value=\"{{#if quantity}}{{quantity}}{{else}}{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\n    min=\"{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}\"\n    {{#if max_purchase_quantity}}max=\"{{max_purchase_quantity}}\"{{/if}}\n    data-quantity-control-input\n  >\n  <span class=\"quantity-control quantity-increment\" data-quantity-control-action=\"increment\">&plus;</span>\n  <span class=\"quantity-control quantity-decrement\" data-quantity-control-action=\"decrement\">&#45;</span>\n</span>\n"}
