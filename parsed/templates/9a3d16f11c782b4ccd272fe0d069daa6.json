{"components/common/share-buttons":"{{#if share_buttons}}\n  <nav class=\"share-buttons-nav\">\n    <h2 class=\"share-heading\"><span>{{lang 'blog.share_this'}}</span></h2>\n    <ul class=\"share-buttons\" data-share-buttons>\n\n      {{#each share_buttons.buttons}}\n\n        {{#if service '===' 'facebook'}}\n          <li class=\"social-link facebook\">\n            <a\n              target=\"_blank\"\n              href=\"http://www.facebook.com/sharer.php?u={{../../url}}\"\n              class=\"facebook\"\n            >\n              <svg class=\"icon-social icon-facebook\">\n                <use xlink:href=\"#icon-facebook\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'twitter'}}\n          <li class=\"social-link twitter\">\n            <a\n              target=\"_blank\"\n              href=\"https://twitter.com/share?url={{../../url}}&amp;text={{../../title}}\"\n              class=\"twitter\"\n            >\n              <svg class=\"icon-social icon-twitter\">\n                <use xlink:href=\"#icon-twitter\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'google_plusone'}}\n          <li class=\"social-link google-plus\">\n            <a\n              target=\"_blank\"\n              href=\"https://plus.google.com/share?url={{../../url}}\"\n              class=\"google\"\n            >\n              <svg class=\"icon-social icon-google\">\n                <use xlink:href=\"#icon-google\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'linkedin'}}\n          <li class=\"social-link linkedin\">\n            <a\n              target=\"_blank\"\n              href=\"http://www.linkedin.com/shareArticle?url={{../../url}}&amp;title={{../../title}}\"\n              class=\"linkedin\"\n            >\n              <svg class=\"icon-social icon-linkedin\">\n                <use xlink:href=\"#icon-linkedin\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'email'}}\n          <li class=\"social-link mail\">\n            <a\n              href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\"\n              class=\"email\"\n            >\n              <svg class=\"icon-social icon-mail\">\n                <use xlink:href=\"#icon-mail\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'print'}}\n          <li class=\"social-link print\">\n            <a\n              class=\"print\"\n              href=\"javascript:;\"\n              onclick=\"window.print()\"\n            >\n              <svg class=\"icon-social icon-print\">\n                <use xlink:href=\"#icon-print\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'pinterest'}}\n          <li class=\"social-link pinterest\">\n            <a\n              target=\"_blank\"\n              href=\"//pinterest.com/pin/create/button/?url={{../../url}}&amp;media={{../../image}}&amp;description={{../../title}}\"\n              class=\"share-pinterest\"\n            >\n              <svg class=\"icon-social icon-pinterest\">\n                <use xlink:href=\"#icon-pinterest\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n      {{/each}}\n      {{#each share_buttons.buttons}}\n\n        {{#if service '===' 'facebook_like'}}\n          <li class=\"social-link facebook-like\">\n            <div class=\"fb-like\"\n              data-href=\"{{../../url}}\"\n              data-layout=\"button_count\"\n              data-action=\"like\"\n              data-show-faces=\"false\">\n            </div>\n          </li>\n        {{/if}}\n\n      {{/each}}\n    </ul>\n  </nav>\n{{/if}}\n"}