{"components/common/quantity":"{{!\n  Quantity Widget with Increment/Decrement controls.\n\n  @param product\n         cart.item or product object\n}}\n\n<span class=\"form-field-quantity-control\" data-quantity-control=\"{{id}}\">\n  <input\n    class=\"form-input\"\n    type=\"number\"\n    name=\"qty{{#if product_id}}-{{id}}{{else}}[]{{/if}}\"\n    pattern=\"[0-9]*\"\n    value=\"{{#if quantity}}{{quantity}}{{else}}{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\n    min=\"{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}\"\n    {{#if max_purchase_quantity}}max=\"{{max_purchase_quantity}}\"{{/if}}\n    data-quantity-control-input\n  >\n  <span class=\"quantity-control quantity-increment\" data-quantity-control-action=\"increment\">&plus;</span>\n  <span class=\"quantity-control quantity-decrement\" data-quantity-control-action=\"decrement\">&#45;</span>\n</span>\n","components/products/price":"{{!\r\n  Shows a product's price.\r\n\r\n  Prices are fairly complicated. Besides having to be able to show the price\r\n  with and without tax, a merchant can also assign a Recommended Retail Price,\r\n  and a Sale Price.\r\n\r\n  This means that in extreme cases, we have to show 4 different prices at once.\r\n\r\n  When a Sale Price is set we do not have direct access to it--instead it\r\n  overwrites the regular price.\r\n\r\n  @param price object\r\n\r\n  @param show_tax_labels bool\r\n         If true, show a label saying the price is with or without tax.\r\n\r\n  @param show_savings bool\r\n         If true, show a label saying how much is saved from the full price.\r\n}}\r\n{{#if price}}\r\n  {{#or has_options product.options.length}}\r\n    <div class=\"price {{#if price.without_tax}}{{#if price.with_tax}}multiple-prices{{/if}}{{/if}}\" {{#if schema_org}} itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\" {{/if}}>\r\n      {{#if schema_org}}\r\n        <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n\r\n        {{#if price.without_tax}}\r\n          <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n        {{else}}\r\n          <meta itemprop=\"price\" content=\"{{price.with_tax.value}}\">\r\n        {{/if}}\r\n      {{/if}}\r\n\r\n      {{#if price.without_tax}}\r\n        <span class=\"price-withouttax\" data-product-price-wrapper=\"without-tax\">\r\n\r\n          {{#if page_type '!==' 'product'}}\r\n            {{#if price_as_low_as}}\r\n              <span>{{lang 'product.as_low_as_price_message'}}</span>\r\n            {{/if}}\r\n          {{/if}}\r\n\r\n          {{#if price.rrp_without_tax}}\r\n            <span class=\"price-rrp\" data-price-rrp>\r\n              {{price.rrp_without_tax.formatted}}\r\n            </span>\r\n          {{/if}}\r\n\r\n          <span class=\"price-value\" data-product-price>\r\n            {{price.without_tax.formatted}}\r\n          </span>\r\n\r\n          {{#if show_tax_labels}}\r\n            {{#if price.with_tax}}\r\n              <span class=\"price-tax-label\">{{lang 'product.excluding_tax'}}</span>\r\n            {{/if}}\r\n          {{/if}}\r\n        </span>\r\n      {{/if}}\r\n\r\n      {{#if price.with_tax}}\r\n        <span class=\"price-withtax\" data-product-price-wrapper=\"with-tax\">\r\n\r\n          {{#if page_type '!==' 'product'}}\r\n            {{#if price_as_low_as}}\r\n              <span>{{lang 'product.as_low_as_price_message'}}</span>\r\n            {{/if}}\r\n          {{/if}}\r\n\r\n          {{#if price.rrp_with_tax}}\r\n            <span class=\"price-rrp\" data-price-rrp>\r\n              {{price.rrp_with_tax.formatted}}\r\n            </span>\r\n          {{/if}}\r\n\r\n          <span class=\"price-value\" data-prodct-price>\r\n            {{price.with_tax.formatted}}\r\n          </span>\r\n\r\n          {{#if show_tax_labels}}\r\n            {{#if price.without_tax}}\r\n              <span class=\"price-tax-label\">{{lang 'product.including_tax'}}</span>\r\n            {{/if}}\r\n          {{/if}}\r\n        </span>\r\n      {{/if}}\r\n\r\n      {{#if show_savings}}\r\n        {{#if price.saved}}\r\n          <span class=\"price-saved\" data-product-price-saved>\r\n            {{lang 'core.price.you_save' amount=price.saved.formatted}}\r\n          </span>\r\n        {{/if}}\r\n      {{/if}}\r\n    </div>\r\n  {{else}}\r\n    <div class=\"price {{#if price.without_tax}}{{#if price.with_tax}}multiple-prices{{/if}}{{/if}}\" {{#if schema_org}} itemprop=\"offers\"\r\n         itemscope itemtype=\"http://schema.org/Offer\" {{/if}} >\r\n      {{#if schema_org}}\r\n      \r\n        {{#if product.can_purchase}}\r\n          <meta itemprop=\"availability\" content=\"http://schema.org/InStock\">\r\n        {{/if}}\r\n        \r\n        <meta itemprop=\"itemCondition\" content=\"http://schema.org/NewCondition\">\r\n        \r\n        <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n\r\n        {{#if price.without_tax}}\r\n          <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n        {{else}}\r\n          <meta itemprop=\"price\" content=\"{{price.with_tax.value}}\">\r\n        {{/if}}\r\n      {{/if}}\r\n\r\n      {{#if price.without_tax}}\r\n        <span class=\"price-withouttax\" data-product-price-wrapper=\"without-tax\">\r\n\r\n          {{#if price.rrp_without_tax}}\r\n            <span class=\"price-rrp\" data-price-rrp>\r\n              {{price.rrp_without_tax.formatted}}\r\n            </span>\r\n          {{/if}}\r\n\r\n          <span class=\"price-value\" data-product-price>\r\n            {{price.without_tax.formatted}}\r\n          </span>\r\n\r\n          {{#if show_tax_labels}}\r\n            {{#if price.with_tax}}\r\n              <span class=\"price-tax-label\">{{lang 'product.excluding_tax'}}</span>\r\n            {{/if}}\r\n          {{/if}}\r\n        </span>\r\n      {{/if}}\r\n\r\n      {{#if price.with_tax}}\r\n        <span class=\"price-withtax\" data-product-price-wrapper=\"with-tax\">\r\n\r\n          {{#if price.rrp_with_tax}}\r\n            <span class=\"price-rrp\" data-price-rrp>\r\n              {{price.rrp_with_tax.formatted}}\r\n            </span>\r\n          {{/if}}\r\n\r\n          <span class=\"price-value\" data-prodct-price>\r\n            {{price.with_tax.formatted}}\r\n          </span>\r\n\r\n          {{#if show_tax_labels}}\r\n            {{#if price.without_tax}}\r\n              <span class=\"price-tax-label\">{{lang 'product.including_tax'}}</span>\r\n            {{/if}}\r\n          {{/if}}\r\n        </span>\r\n      {{/if}}\r\n\r\n      {{#if show_savings}}\r\n        {{#if price.saved}}\r\n          <!--<span class=\"price-saved\" data-product-price-saved>\r\n            {{lang 'core.price.you_save' amount=price.saved.formatted}}\r\n          </span>-->\r\n        {{/if}}\r\n      {{/if}}\r\n    </div>\r\n  {{/or}}\r\n{{/if}}\r\n{{#if call_for_price}}\r\n  <div class=\"price call-for-price\">\r\n    {{call_for_price}}\r\n  </div>\r\n{{/if}}\r\n","components/products/product-details":"\r\n<div data-product-details data-product-title=\"{{product.title}}\">\r\n\r\n  <form\r\n    class=\"form product-form\"\r\n    method=\"post\"\r\n    action=\"{{product.cart_url}}\"\r\n    data-product-options-count=\"{{product.options.length}}\"\r\n    enctype=\"multipart/form-data\"\r\n    data-cart-item-add>\r\n    <input type=\"hidden\" name=\"action\" value=\"add\">\r\n    <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" data-product-id>\r\n\r\n    {{#gt product.num_reviews '0'}}\r\n    {{#if settings.show_product_rating}}\r\n      <div itemprop=\"aggregateRating\" itemscope itemtype=\"http://schema.org/AggregateRating\">\r\n        <meta itemprop=\"ratingValue\" content=\"{{product.rating}}\">\r\n        <meta itemprop=\"reviewCount\" content=\"{{product.num_reviews}}\">\r\n      </div>\r\n    {{/if}}\r\n    {{/gt}}\r\n\r\n    {{#if product.brand.name.length}}\r\n      {{#if product.brand.url}}\r\n        <a class=\"product-vendor\" href=\"{{product.brand.url}}\">{{product.brand.name}}</a>\r\n      {{else}}\r\n        <span class=\"product-vendor\">{{product.brand.name}}</span>\r\n      {{/if}}\r\n    {{/if}}\r\n\r\n    <h1 class=\"page-title\" itemprop=\"name\">{{product.title}}</h1>\r\n\r\n    {{# if product.num_reviews \">\" 0}}\r\n      <div class=\"form-field product-rating\">\r\n        {{> components/products/ratings rating=product.rating aggregate=true}}\r\n        {{{snippet 'product_rating'}}}\r\n      </div>\r\n    {{/if}}\r\n\r\n    {{> components/products/price price=product.price show_savings=true schema_org=true show_tax_labels=true call_for_price=product.call_for_price}}\r\n\r\n    {{#if product.show_quantity_input}}\r\n      <div class=\"form-field product-quantity-wrapper\" data-product-quantity>\r\n        <label class=\"form-label\">\r\n          <span class=\"form-field-title\">{{lang 'product.quantity'}}</span>\r\n          {{> components/common/quantity product}}\r\n        </label>\r\n      </div>\r\n    {{/if}}\r\n\r\n    {{{snippet 'product_details'}}}\r\n\r\n    <div class=\"product-info\">\r\n      {{#if product.sku}}\r\n        <div class=\"product-info-item product-info-sku\">\r\n          <span class=\"product-info-label\">{{lang 'product.sku'}}</span>\r\n          <span class=\"product-info-value\" data-product-sku>{{product.sku}}</span>\r\n        </div>\r\n      {{/if}}\r\n\r\n      {{#if product.weight}}\r\n        <div class=\"product-info-item product-info-weight\">\r\n          <span class=\"product-info-label\">{{lang 'product.weight'}}</span>\r\n          <span class=\"product-info-value\" data-product-weight>{{product.weight}}</span>\r\n        </div>\r\n      {{/if}}\r\n\r\n      {{#if theme_settings.product-show-dimensions}}\r\n        {{#if product.height}}\r\n          <div class=\"product-info-item product-info-height\">\r\n            <span class=\"product-info-label\">{{lang 'product.height'}}</span>\r\n            <span class=\"product-info-value\" data-product-height>\r\n              {{{lang 'product.height_value' height=product.height uom=settings.measurements.length}}}\r\n            </span>\r\n          </div>\r\n        {{/if}}\r\n\r\n        {{#if product.width}}\r\n          <div class=\"product-info-item product-info-width\">\r\n            <span class=\"product-info-label\">{{lang 'product.width'}}</span>\r\n            <span class=\"product-info-value\" data-product-width>\r\n              {{{lang 'product.width_value' width=product.width uom=settings.measurements.length}}}\r\n            </span>\r\n          </div>\r\n        {{/if}}\r\n\r\n        {{#if product.depth}}\r\n          <div class=\"product-info-item product-info-depth\">\r\n            <span class=\"product-info-label\">{{lang 'product.depth'}}</span>\r\n            <span class=\"product-info-value\" data-product-depth>\r\n              {{{lang 'product.depth_value' depth=product.depth uom=settings.measurements.length}}}\r\n            </span>\r\n          </div>\r\n        {{/if}}\r\n      {{/if}}\r\n\r\n      {{#if product.availability}}\r\n        <div class=\"product-info-item product-info-availability\">\r\n          <span class=\"product-info-label\">{{lang 'product.availability'}}</span>\r\n          <span class=\"product-info-value\" data-product-availability>{{product.availability}}</span>\r\n        </div>\r\n      {{/if}}\r\n\r\n      {{#if product.min_purchase_quantity}}\r\n        <div class=\"product-info-item product-info-min-purchase-quantity\">\r\n          <span class=\"product-info-label\">{{lang 'product.min_purchase'}}</span>\r\n          <span class=\"product-info-value\" data-product-min-purchase-quantity>{{product.min_purchase_quantity}}</span>\r\n        </div>\r\n      {{/if}}\r\n\r\n      {{#if product.max_purchase_quantity}}\r\n        <div class=\"product-info-item product-info-max-purchase-quantity\">\r\n          <span class=\"product-info-label\">{{lang 'product.max_purchase'}}</span>\r\n          <span class=\"product-info-value\" data-product-max-purchase-quantity>{{product.max_purchase_quantity}}</span>\r\n        </div>\r\n      {{/if}}\r\n\r\n      {{#if product.stock_level}}\r\n        <div class=\"product-info-item product-info-stock-level\">\r\n          <span class=\"product-info-label\">{{lang 'product.stock_level'}}</span>\r\n          <span class=\"product-info-value\" data-product-stock-level>{{product.stock_level}}</span>\r\n        </div>\r\n      {{/if}}\r\n\r\n      {{#if product.shipping}}\r\n        {{#if product.shipping.calculated}}\r\n          <div class=\"product-info-item product-info-shipping-calculated\">\r\n            <span class=\"product-info-label\">{{lang 'product.shipping'}}</span>\r\n            <span class=\"product-info-value\">{{lang 'product.shipping_calculated'}}</span>\r\n          </div>\r\n        {{else}}\r\n          {{#if product.shipping.price '===' 0}}\r\n            <div class=\"product-info-item product-info-shipping-free\">\r\n              <span class=\"product-info-label\">{{lang 'product.shipping'}}</span>\r\n              <span class=\"product-info-value\">{{lang 'product.shipping_free'}}</span>\r\n            </div>\r\n          {{else}}\r\n            {{#if product.shipping.fixed}}\r\n              <div class=\"product-info-item product-info-shipping-fixed\">\r\n                <span class=\"product-info-label\">{{lang 'product.shipping'}}</span>\r\n                <span class=\"product-info-value\">{{lang 'product.shipping_fixed' amount=product.shipping.price.formatted}}</span>\r\n              </div>\r\n            {{/if}}\r\n          {{/if}}\r\n        {{/if}}\r\n      {{/if}}\r\n\r\n      {{#if product.gift_wrapping_available}}\r\n        <div class=\"product-info-item product-info-gift-wrapping\">\r\n          <span class=\"product-info-label\">{{lang 'product.gift_wrapping'}}</span>\r\n          <span class=\"product-info-value\" data-product-gift-wrapping>{{lang 'product.gift_wrapping_available'}}</span>\r\n        </div>\r\n      {{/if}}\r\n\r\n      {{#each product.custom_fields}}\r\n        {{#if name '!==' 'Fragrance'}}\r\n          {{#if name '!==' 'is_bundle'}}\r\n            <div class=\"product-info-item product-info-custom-field\">\r\n              <span class=\"product-info-label\">{{name}}:</span>\r\n              <span class=\"product-info-value\">{{value}}</span>\r\n            </div>\r\n          {{/if}}\r\n        {{/if}}\r\n      {{/each}}\r\n\r\n      {{#if product.warranty}}\r\n        <div class=\"product-info-item product-info-warranty\">\r\n          <span class=\"product-info-label\">{{lang 'product.warranty'}}</span>\r\n          <span class=\"product-info-value\">{{nl2br product.warranty}}</span>\r\n        </div>\r\n      {{/if}}\r\n\r\n      {{#if product.bulk_discount_rates.length}}\r\n        <div class=\"product-info-item product-info-bulk-pricing\">\r\n          <span class=\"product-info-label\">{{lang 'product.bulk_pricing.title'}}</span>\r\n          <span class=\"product-info-value\">\r\n            <a href=\"#\" class=\"modal-toggle product-bulk-pricing-toggle\">{{lang 'product.bulk_pricing.see_pricing'}}</a>\r\n          </span>\r\n        </div>\r\n\r\n        <div class=\"product-bulk-pricing-modal\">\r\n          <h3 class=\"modal-title\">{{lang 'product.bulk_pricing.title'}}</h3>\r\n          <p>{{lang 'product.bulk_pricing.instructions'}}</p>\r\n          <ul>\r\n            {{#each product.bulk_discount_rates}}\r\n              <li>\r\n                {{lang 'product.bulk_pricing.range' min=min max=max}}\r\n                {{#if type \"===\" \"percent\"}}\r\n                  {{lang 'product.bulk_pricing.percent' discount=discount.formatted}}\r\n                {{/if}}\r\n                {{#if type \"===\" \"fixed\"}}\r\n                  {{lang 'product.bulk_pricing.fixed' discount=discount.formatted}}\r\n                {{/if}}\r\n                {{#if type \"===\" \"price\"}}\r\n                  {{lang 'product.bulk_pricing.price' discount=discount.formatted}}\r\n                {{/if}}\r\n              </li>\r\n            {{/each}}\r\n          </ul>\r\n        </div>\r\n      {{/if}}\r\n    </div>\r\n\r\n    {{#if product.options}}\r\n      <div class=\"product-options\" data-product-option-change>\r\n        {{#each product.options}}\r\n          {{{dynamicComponent 'core/forms/options'}}}\r\n        {{/each}}\r\n      </div>\r\n    {{/if}}\r\n\r\n    {{#if product.event_date}}\r\n      {{> core/products/event}}\r\n    {{/if}}\r\n\r\n    {{#if product.customizations}}\r\n      <div class=\"product-customizations\" data-product-option-change>\r\n        {{#each product.customizations}}\r\n          {{{dynamicComponent 'core/forms/customizations'}}}\r\n        {{/each}}\r\n      </div>\r\n    {{/if}}\r\n\r\n    {{#if product.release_date}}\r\n      <div class=\"form-field pre-order-message\">\r\n        {{product.release_date}}\r\n      </div>\r\n    {{/if}}\r\n\r\n    {{#if product.can_purchase}}\r\n      <div class=\"form-field form-submit-wrapper\" data-product-add>\r\n        {{#if product.pre_order}}\r\n          <input class=\"button pre-order\" type=\"submit\" value=\"{{lang 'product.pre_order'}}\" data-button-purchase data-button-preorder>\r\n        {{else}}\r\n          <input class=\"button\" type=\"submit\" value=\"{{lang 'product.add_to_cart'}}\" data-button-purchase>\r\n        {{/if}}\r\n\r\n        {{#if settings.show_wishlist}}\r\n          <a class=\"button inverted\" href=\"{{product.add_to_wishlist_url}}\" data-wishlist-add>{{lang 'product.add_to_wishlist'}}</a>\r\n        {{/if}}\r\n\r\n        <span class=\"loading\"></span>\r\n      </div>\r\n      {{{snippet 'product_addtocart'}}}\r\n\r\n      <div class=\"product-alert\" data-product-cart-message></div>\r\n      <div class=\"product-alert\" data-product-option-message></div>\r\n    {{else}}\r\n    \r\n        {{#if product.id \"===\" \"407\"}}\r\n        <a class=\"button\" href=\"/original-home-fragrance-spray/\">See the newer version</a>\r\n        {{else}}\r\n        <input type=\"button\" disabled=\"disabled\" value=\"Sold Out\" class=\"button disabled\" />\r\n        {{/if}}\r\n      {{/if}}\r\n  </form>\r\n</div>\r\n","components/products/ratings":"\r\n<div class=\"product-rating\">\r\n  <div class=\"rating-stars-readonly\" aria-hidden=\"true\">\r\n    <svg class=\"rating-stars\" viewBox=\"0 0 200 40\"> \r\n        {{#for 1 5}}\r\n          <use {{#if ../rating '>=' $index}}xlink:href=\"#full-star\" href=\"#full-star\" {{else}} xlink:href=\"#empty-star\" href=\"#empty-star\"{{/if}} x=\"{{ multiply (subtract $index 3) 40}}\"></use>\r\n        {{/for}}\r\n    </svg>\r\n  </div>\r\n  <span class=\"rating-total-score sr-only\">{{this.rating}}</span>\r\n  {{#if aggregate}}\r\n    <a class=\"rating-total-votes\" href=\"#user-reviews\">({{product.num_reviews}})</a>\r\n  {{/if}}\r\n</div>\r\n","core/forms/customizations/checkbox":"\n<div class=\"form-field form-field-customizations form-field-checkbox{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-control\">\n      <input class=\"form-input form-checkbox\"\n        id=\"product-field-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"checkbox\"\n        {{#if value}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text product-customization-text\">\n        {{display_name}}\n        {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/file":"\n<div class=\"form-field form-field-customizations form-field-file {{#if required}}form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/customizations/select":"\n<div class=\"form-field form-field-customizations form-field-select{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper\">\n        <select\n          class=\"form-input input-select\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\" {{#any values selected=true}}{{else}}selected{{/any}} disabled>{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{this}}\" {{#if selected}}selected{{/if}}>{{this}}</option>\n          {{/each}}\n        </select>\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/text":"\n<div class=\"form-field form-field-customizations form-field-text{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\" for=\"product-fields-{{id}}\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"{{value}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/customizations/textarea":"\n<div class=\"form-field form-field-customizations form-field-textarea{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}:\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n","core/forms/options/date":"<div class=\"form-field form-field-options form-field-date{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"date\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][year]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.year'}}</option>\n          {{#if earliest_year '===' latest_year}}\n