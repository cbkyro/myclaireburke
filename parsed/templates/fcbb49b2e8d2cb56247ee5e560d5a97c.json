{"components/account-links":"{{#if settings.account_creation_enabled}}\r\n  <span class=\"account-links\">\r\n    {{#if customer}}\r\n      {{!--<a href=\"{{urls.account.index}}\">{{lang 'header.logged_in_as' name=customer.name}}</a>\r\n      <span class=\"account-link-separator\">&ndash;</span>--}}\r\n      <a href=\"{{urls.auth.logout}}\">{{> components/common/icons/login}} <span>{{lang 'header.log_out'}}</span></a>\r\n    {{else}}\r\n      {{!--<a href=\"{{urls.auth.create_account}}\">{{lang 'header.sign_up'}}</a>\r\n      <span class=\"account-link-separator\">&ndash;</span>--}}\r\n      <a href=\"{{urls.auth.login}}\">{{> components/common/icons/login}} <span>{{lang 'header.log_in'}}</span></a>\r\n    {{/if}}\r\n  </span>\r\n{{/if}}\r\n","components/brand/logo":"<div class=\"branding\">\r\n  {{#if settings.store_logo.image.data}}\r\n    <a class=\"logo\" href=\"/\">\r\n      <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image}}\" alt=\"{{settings.store_logo.image.alt}}\">\r\n    </a>\r\n  {{else}}\r\n    <h1 class=\"text\"><a href=\"/\">{{settings.store_logo.title}}</a></h1>\r\n  {{/if}}\r\n</div>\r\n","components/cart-preview":"<div class=\"cart-preview\" id=\"cart-preview\" data-count=\"{{cart.quantity}}\">\n\n  {{#if cart.items}}\n    <div class=\"cart-preview-body\">\n\n      <ul class=\"cart-preview-list\">\n        {{#each cart.items}}\n          <li class=\"cart-preview-list-item\">\n\n            <a class=\"cart-preview-list-item-remove\" data-product-id=\"{{id}}\" href=\"{{remove_url}}\">&times;</a>\n\n            {{#if image}}\n              <div class=\"cart-preview-list-item-image-wrap\">\n                <div class=\"cart-preview-list-item-image\">\n                  <a href=\"{{url}}\">\n                    <img src=\"{{getImage image 'thumbnail' default_image}}\"/>\n                  </a>\n                </div>\n              </div>\n            {{else}}\n              <div class=\"cart-preview-list-item-image-wrap\">\n                <div class=\"cart-preview-list-item-image\">\n                  <a href=\"{{url}}\">\n                    <img src=\"{{getImage image 'thumbnail' ../../theme_settings.default_image}}\"/>\n                  </a>\n                </div>\n              </div>\n            {{/if}}\n\n            <div class=\"cart-preview-list-item-details\">\n\n              <div class=\"cart-preview-list-item-brand\">\n                {{brand.name}}\n              </div>\n\n              <div class=\"cart-preview-list-item-name\">\n                <a href=\"{{url}}\" alt=\"{{name}}\">{{name}}</a>\n              </div>\n\n              {{#if options.length}}\n                <div class=\"cart-preview-list-options\">\n                  {{#each options}}\n                    {{name}}:&nbsp;{{value}}{{#unless @last}}, {{/unless}}\n                  {{/each}}\n                </div>\n              {{/if}}\n\n              <div class=\"cart-preview-list-item-price\">\n                <span class=\"{{#if price_discounted}}price-discounted{{/if}}\">{{price.formatted}}</span>\n                {{#if price_discounted}}\n                  <span class=\"cart-item-discount\">{{price_discounted.formatted}}</span>\n                {{/if}}\n                <span class=\"multiplier\"> &times;</span> {{quantity}}\n              </div>\n            </div>\n\n          </li>\n        {{/each}}\n      </ul>\n\n    </div>\n\n    <div class=\"cart-preview-footer\">\n      {{#if cart.discount.value \"<\" 0}}\n        <div class=\"cart-preview-discount\">\n          <div class=\"cart-preview-total-price\">\n            {{lang 'cart.discount'}} {{cart.discount.formatted}}\n          </div>\n        </div>\n      {{/if}}\n      {{#if cart.taxes}}\n        <div class=\"cart-preview-taxes\">\n          {{#each cart.taxes}}\n          <div class=\"cart-preview-tax\">\n            <div class=\"cart-preview-total-price\">\n              {{name}} {{cost.formatted}}\n            </div>\n          </div>\n          {{/each}}\n        </div>\n      {{/if}}\n      <div class=\"cart-preview-pricing\">\n        <p class=\"cart-preview-total-price\">{{lang 'header.total'}} &nbsp; {{{cart.grand_total.formatted}}}</p>\n      </div>\n      <a href=\"{{urls.cart}}\" class=\"button secondary\"><span>{{lang 'header.view_cart'}}</span></a>\n      <a href=\"{{urls.checkout.single_address}}\" class=\"button\">{{lang 'cart_preview.checkout.title'}}</a>\n    </div>\n\n  {{else}}\n    <p class=\"cart-preview-empty\">{{lang 'cart_preview.checkout.empty_cart'}}</p>\n  {{/if}}\n  <div id=\"cart-preview-overlay\" class=\"cart-preview-overlay\"></div>\n</div>\n","components/common/icons/cart":"<svg version=\"1.1\"\n\t xmlns=\"http://www.w3.org/2000/svg\"\n\t x=\"0px\" y=\"0px\" width=\"24.3px\" height=\"28.5px\" viewBox=\"0 0 24.3 28.5\">\n\t\t<path class=\"st0\" d=\"M0,28.5c0.4-7.3,0.8-14.6,1.2-21.9c1.8,0,3.6,0,5.4,0c0,0,0-0.1,0-0.1c0-0.5,0-1,0-1.4c0-0.6,0.2-1.2,0.4-1.7\n\t\t\tc0.3-0.6,0.7-1.2,1.1-1.7c0.4-0.4,0.8-0.7,1.3-1c0.6-0.3,1.3-0.5,2-0.6c0.7-0.1,1.4,0,2.1,0.2c0.8,0.2,1.5,0.6,2.1,1.2\n\t\t\tc0.6,0.5,1.1,1.2,1.4,2c0.3,0.7,0.4,1.4,0.4,2.2c0,0.4,0,0.7,0,1.1c1.8,0,3.6,0,5.4,0c0.4,7.3,0.8,14.6,1.2,21.9\n\t\t\tC16.2,28.5,8.1,28.5,0,28.5z M21,8.8c-1.1,0-2.2,0-3.4,0c0,1.1,0,2.2,0,3.3c-0.7,0-1.5,0-2.2,0c0-1.1,0-2.2,0-3.3\n\t\t\tc-2.2,0-4.4,0-6.6,0c0,1.1,0,2.2,0,3.3c-0.7,0-1.5,0-2.2,0c0-1.1,0-2.2,0-3.3c-1.1,0-2.2,0-3.3,0c-0.3,5.9-0.6,11.7-1,17.5\n\t\t\tc6.5,0,13.1,0,19.6,0C21.6,20.5,21.3,14.6,21,8.8z M15.4,6.6c0-0.4,0-0.8,0-1.2c0-0.2,0-0.4-0.1-0.7c-0.4-1.4-1.2-2.2-2.6-2.5\n\t\t\tc-0.7-0.2-1.5,0-2.1,0.3C9.9,3,9.4,3.6,9.1,4.3C8.8,5.1,8.9,5.8,8.8,6.6C11,6.6,13.2,6.6,15.4,6.6z\"/>\n</svg>","components/common/icons/down-arrow":"<svg width=\"7px\" height=\"4px\" viewBox=\"0 0 7 4\" version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" xml:space=\"preserve\">\n  <polygon points=\"0 0 3.5 4 7 0 \" fill=\"#000000\"/>\n</svg>\n","components/common/icons/locat