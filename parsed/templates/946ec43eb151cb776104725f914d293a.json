{"components/common/newsletter-subscribe":"<form\n  class=\"form subscription-form\"\n  action=\"{{urls.subscribe.action}}\"\n  method=\"post\"\n>\n  <div class=\"form-field form-inline subscription-form-email\">\n    <label class=\"form-label\" for=\"newsletter-subscribe\">\n      {{lang 'newsletter.subscribe'}}\n    </label>\n    <input\n      class=\"form-input\"\n      id=\"newsletter-subscribe\"\n      type=\"email\"\n      name=\"nl_email\"\n      value=\"{{customer.email}}\"\n      placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n    >\n\n    {{! TODO: Make sure the name/check fields are needed}}\n    <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n    <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n    <input type=\"hidden\" name=\"check\" value=\"1\">\n\n    <input\n      class=\"button subscription-form-submit\"\n      type=\"submit\"\n      value=\"{{lang 'newsletter.subscribe_submit'}}\"\n    >\n  </div>\n</form>\n"}