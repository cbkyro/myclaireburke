{"components/faceted-search/facets/rating":"<ul class=\"facet-option facet-rating {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\" data-facet-filter-wrapper>\n  {{#each items}}\n    <li data-facet-item>\n      <a class=\"facet-select {{#if selected}}checked{{/if}}\" href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n        {{> components/products/ratings rating=param_value}}\n        {{#if ../show_product_counts}}<span data-facet-item-count>({{count}})</span>{{/if}}\n      </a>\n    </li>\n  {{/each}}\n</ul>\n","components/products/ratings":"\r\n<div class=\"product-rating\">\r\n  <div class=\"rating-stars-readonly\" aria-hidden=\"true\">\r\n    <svg class=\"rating-stars\" viewBox=\"0 0 200 40\"> \r\n        {{#for 1 5}}\r\n          <use {{#if ../rating '>=' $index}}xlink:href=\"#full-star\" href=\"#full-star\" {{else}} xlink:href=\"#empty-star\" href=\"#empty-star\"{{/if}} x=\"{{ multiply (subtract $index 3) 40}}\"></use>\r\n        {{/for}}\r\n    </svg>\r\n  </div>\r\n  <span class=\"rating-total-score sr-only\">{{this.rating}}</span>\r\n  {{#if aggregate}}\r\n    <a class=\"rating-total-votes\" href=\"#user-reviews\">({{product.num_reviews}})</a>\r\n  {{/if}}\r\n</div>\r\n"}
